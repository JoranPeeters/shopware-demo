{
    "name": "my-php-unit-plugin/my-php-unit-plugin",
    "description": "Test a simple command with phpunit",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "shopware/core": "~6.6.0"
    },
    "extra": {
        "shopware-plugin-class": "MyPhpUnitPlugin\\MyPhpUnitPlugin",
        "label": {
            "de-DE": "Skeleton plugin",
            "en-GB": "Skeleton plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "MyPhpUnitPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyPhpUnitPlugin\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
